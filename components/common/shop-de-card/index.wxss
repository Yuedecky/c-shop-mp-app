.sh-de-container{
    display: flex;
    flex-direction: column;
    padding: 20rpx;
}

.sh-de-header{
    line-height:60rpx;
    font-weight: bold;
}
.sh-de-open-date{
    line-height: 60rpx;
    font-size:26rpx;
    color:#ccc;
}

.sh-de-address{
    color:#666;
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    margin-bottom: 20rpx;
}

.sh-de-phone{
    color: #09f;
    font-size:26rpx;
}
