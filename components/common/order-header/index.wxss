.order-h-container{
    display: flex;
    flex-direction: column;
    align-items: center;
}

.order-h-header{
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    font-size: 40rpx;
    line-height: 80rpx;
}

.order-image-wrapper{
    width:120rpx;
    height:120rpx;
    display: flex;
    flex-direction: column;
    align-content: center;

}

.order-image-info{
    width: 100rpx;
    margin: 0 auto;
    height: 100rpx;
}

.order-h-body{
    display: flex;
    flex-direction: row;
    justify-content: space-between;
}

.order-body-left{
    margin-right: 100rpx;
}

